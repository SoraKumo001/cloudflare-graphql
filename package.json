{
	"name": "cloudflare-graphql",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "npm-run-all -p dev:*",
		"dev:yoga": "wrangler dev --minify",
		"dev:docker": "docker compose -f docker/development/docker-compose.yml up -d",
		"dev:proxy": "prisma-accelerate-local postgresql://postgres:password@localhost:25432/postgres -p 8000 -t",
		"start": "wrangler dev",
		"prisma:generate": "prisma generate && tsx tools/fix-runtimeDataModel",
		"prisma:migrate": "prisma migrate dev",
		"deploy": "wrangler deploy",
		"prisma:generate:migrate": "next-exec -c production -- prisma migrate deploy"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240312.0",
		"@types/pg": "^8.11.2",
		"next-exec": "^1.0.0",
		"npm-run-all": "^4.1.5",
		"prisma": "^5.11.0",
		"prisma-accelerate-local": "^1.1.2",
		"typescript": "^5.4.2",
		"wrangler": "^3.34.1"
	},
	"dependencies": {
		"@pothos/core": "^3.41.0",
		"@pothos/plugin-prisma": "^3.65.0",
		"@pothos/plugin-prisma-utils": "^0.14.0",
		"@prisma/adapter-pg": "^5.11.0",
		"@prisma/client": "^5.11.0",
		"graphql-yoga": "^5.2.0",
		"pg": "^8.11.3",
		"pothos-prisma-generator": "^1.0.2",
		"tsx": "^4.7.1"
	}
}
